@ IN SOA carlosrm-linux.carlosrm.edu. root.carlosrm.edu. (
					1 ; serial no
					360000 ; refresh
					3600 ; retry
					3600000 ; expire
					3600 ; default ttl
					)
					IN NS carlosrm-linux.carlosrm.edu.
carlosrm-linux.carlosrm.edu.		IN A 192.168.12.2
virtual1.carlosrm.edu.			IN A 192.168.12.2
virtual2.carlosrm.edu.                  IN A 192.168.12.2
otro.carlosrm.edu.			IN CNAME virtual1.carlosrm.edu.
